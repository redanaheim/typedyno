{
  "name": "typedyno",
  "version": "0.1.0",
  "description": "Discord chatbot",
  "main": "out/main.js",
  "type": "module",
  "keywords": [
    "discord"
  ],
  "scripts": {
    "start": "node --inspect out/main.js",
    "ts-build": "tsc && node path_modify.js",
    "heroku-postbuild": "tsc"
  },
  "engines": {
    "node": ">=16.8.0"
  },
  "author": "tigergold59",
  "license": "MIT",
  "dependencies": {
    "@types/ws": "^7.4.6",
    "chalk": "^4.1.0",
    "discord.js": "^12.5.3",
    "paste.ee": "^1.0.1",
    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/node": ">=16.7.4",
    "@types/pg": "^7.14.11",
    "typescript": ">=4.3.5"
  },
  "prettier": {
    "arrowParens": "avoid",
    "bracketSpacing": true,
    "embeddedLanguageFormatting": "off",
