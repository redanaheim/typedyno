{
    "name": "typedyno",
    "version": "0.1.0",
    "description": "Discord chatbot",
    "main": "out/main.js",
    "type": "module",
    "keywords": [
        "discord"
    ],
    "scripts": {
        "start": "node out/main.js",
        "ts-build": "tsc && node path_modify.js",
        "heroku-postbuild": "tsc"
    },
    "engines": {
        "node": ">=16.8.0"
    },
    "author": "tigergold59",
    "license": "MIT",
    "dependencies": {
        "chalk": "^4.1.0",
        "discord.js": "^12.5.3",
        "paste.ee": "^1.0.1",
        "pg": "^8.5.1",
        "reflect-metadata": "^0.1.13"
    },
    "devDependencies": {
        "@types/ws": "^7.4.6",
        "@types/chalk": "^2.2.0",
        "@types/node": ">=16.7.4",
        "@types/pg": "^8.5.1",
        "@typescript-eslint/eslint-plugin": "^4.31.0",
        "@typescript-eslint/parser": "^4.31.0",
        "eslint": "^7.32.0",
        "eslint-plugin-ts-imports": "^0.0.0",
        "typescript": ">=4.3.5"
    },
    "prettier": {
        "arrowParens": "avoid",
        "bracketSpacing": true,
        "embeddedLanguageFormatting": "off",
        "singleQuote": false,
        "trailingComma": "all",
        "semi": true,
        "tabWidth": 4,
        "quoteProps": "consistent",
        "printWidth": 150
    }
}